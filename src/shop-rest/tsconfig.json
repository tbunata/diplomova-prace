  "compilerOptions": {                        
    "target": "esnext",                               
    "module": "commonjs",                           
    "outDir": "./dist",                             
    "rootDir": "./src",                             
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "typeRoots": ["./src/types", "./node_modules/@types"]                       
  },
  "exclude":[
    "./node_modules"
  ]
}